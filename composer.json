{
    "name": "ifdd/services",
    "type": "project",
    "description": "IFDD Services",
    "keywords": [
        "framework",
        "laravel",
        "ifdd",
        "api",
        "services"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "doctrine/dbal": "^3.3",
        "ejarnutowski/laravel-api-key": "^1.0",
        "filament/filament": "^2.13",
        "fruitcake/laravel-cors": "^3.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "http-interop/http-factory-guzzle": "^1.2",
        "laravel/framework": "^9.0",
        "laravel/passport": "^10.3",
        "laravel/sanctum": "^3.0",
        "laravel/scout": "^9.4",
        "laravel/tinker": "^2.5",
        "meilisearch/meilisearch-php": "^0.23.3",
        "psr/simple-cache": "1.0",
        "pxlrbt/filament-excel": "^1.1",
        "shuvroroy/filament-spatie-laravel-backup": "^1.2",
        "shuvroroy/filament-spatie-laravel-health": "^1.6"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^1.9.1",
        "knuckleswtf/scribe": "^3.25",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
